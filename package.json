{
  "name": "wa-mcp-server",
  "version": "1.0.0",
  "description": "MCP server for Web Awesome components",
  "main": "dist/index.js",
  "type": "module",
  "scripts": {
    "build": "tsc",
    "start": "node dist/index.js",
    "dev": "tsx src/index.ts",
    "test": "jest",
    "lint": "eslint src/**/*.ts",
    "fetch-data": "tsx scripts/fetch-components.ts",
    "docker:build": "docker build -t wa-mcp-server .",
    "docker:run": "docker run -p 3000:3000 wa-mcp-server"
  },
  "keywords": [
    "mcp",
    "web-awesome",
    "components"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "@modelcontextprotocol/sdk": "^0.5.0",
    "@types/express": "^5.0.5",
    "express": "^5.1.0",
    "gray-matter": "^4.0.3"
  },
  "devDependencies": {
    "@types/get-port": "^4.0.1",
    "@types/jest": "^29.5.12",
    "@types/node": "^20.11.24",
    "@types/node-fetch": "^2.6.13",
    "@typescript-eslint/eslint-plugin": "^7.1.1",
    "@typescript-eslint/parser": "^7.1.1",
    "eslint": "^8.57.0",
    "get-port": "^7.1.0",
    "jest": "^29.7.0",
    "node-fetch": "^2.7.0",
    "ts-jest": "^29.1.2",
    "tsx": "^4.7.1",
    "typescript": "^5.3.3"
  }
}
